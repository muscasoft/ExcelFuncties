=LAMBDA(mijnTeZoekenTekstInRij;mijnTeDoorzoekenTekstenInRij;
     BYROW(mijnTeZoekenTekstInRij;
          LAMBDA(TeZoekenTekst;
               LET(
                    mijnVindAlles;VIND_ALLES_WAARDE_IN_RIJ(TeZoekenTekst;mijnTeDoorzoekenTekstenInRij);
                    VindAllesAantal;SOM(MijnVindAlles);
                    VindAllesIndex;VERGELIJKEN(1;MijnVindAlles;0);
                    SCHAKELEN(VindAllesAantal;0;0;1;VindAllesIndex;-VindAllesAantal)))))
